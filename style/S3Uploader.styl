.s3PhotoUpload
	width 200px
	.photo
		position relative
		width 200px
		height 200px
		background gray
		border-radius 4px
		margin 10px
		overflow hidden
		img.preview
			width 100%
	.progress
		position absolute
		bottom 0
		left 0
		width 100%
		height 15px
		border-radius 0 0 4px 4px
		.bar
			height 15px
			background red
	.delete
		position absolute
		top 5px
		right 5px